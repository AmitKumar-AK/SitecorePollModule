SELECT Data, DataCode as Integer, count(DataCode) as VotesCount     
	FROM PageEvents
	WHERE PageEventDefinitionId = @PageEventDefinitionId
	and DataKey=@DataKey
    and PageEvents.Timestamp >= @StartDateTicks 
    and	PageEvents.Timestamp <= @EndDateTicks 
	group by Data, DataCode
    order by DataCode
